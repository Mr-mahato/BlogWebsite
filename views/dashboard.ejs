<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link rel="stylesheet" href="navbarStyle.css" />
  </head>
  <body>
    <%- include('navbar' , {role , username})%>
    <h1>Dashboard page</h1>

    
    <% if (blogData.length != 0) { %>
      
      <% blogData[0].blogImage %>
      <% blogData.forEach(function(user){ %>
        <div class="blog">
          <h2><%= user.blogName %></h2>
          <img src="/<%= user.blogImage %>" alt="this is image">
        </div>
      <% }) %>

    <% } %>
    
    


    <script>
      const drop_down = document.querySelector(".drop-down");
      const username = document.querySelector(".showUsername");
      username.addEventListener("click", () => {
        drop_down.style.display = drop_down.style.display === "none" ? "flex" : "none";
      });
    </script>
  </body>
</html>
